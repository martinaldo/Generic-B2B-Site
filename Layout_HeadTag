<script type="text/javascript">
	$(function(){
		$('#cloudzoom_1').bind('click',function(){
			var cloudZoom = $(this).data('CloudZoom');
			cloudZoom.closeZoom();
			$.fancybox.open(cloudZoom.getGalleryList());
			return false;
		});
	});
	CloudZoom.quickStart();
</script>


